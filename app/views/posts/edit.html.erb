<div id="left-col" class="col l4">
  <h5>Admin Account</h5>
  <p><%= link_to "see published post", post_path(@post) %></p>
  <%= button_to "delete #{@post.title}", @post, :method => :delete, data: {confirm: "Are you sure you want to delete this post?"} %>

</div>

<div id="right-col" class='col l6'>
  <h5>Edit Post</h5>
  <%= render partial: 'form' %>
  <br></br>
</div>
