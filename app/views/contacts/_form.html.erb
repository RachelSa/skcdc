<%= form_for @contact do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :ext %>
  <%= f.text_field :ext %>

  <%= f.submit 'submit', :class => "amber lighten-1 white-text btn"data: { confirm:
    "Ready to post this update? All changes will appear on the live SKCDC website." } %> <br></br>
<% end %>
