<div id="left-col" class="col l4 m4 s12 center-align">
  <h5><%= link_to "Admin Account", admin_path %></h5>
  <hr class="yellow-rule">
  Is <%= @contact.first_name %> no longer a contact?<br>
  <%= button_to "delete contact", @contact, :method => :delete, data: {confirm: "Are you sure you want to delete this contact?"}, class: "amber lighten-1 white-text btn" %>
</div>

<div id="right-col" class='col l8 m8 s12'>
  <%= render partial: '/general/error_notice' %>
  <h5 class="yellow-title">Edit <%= @contact.title %></h5>
    <%= render partial: 'form' %>
</div>
