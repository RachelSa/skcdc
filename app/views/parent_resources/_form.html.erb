<%= form_for @parent_resource do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :url %>
  <%= f.text_field :outside_url %>
  <%= f.label :description %>
  <%= f.text_area :description %><br></br>

  <%= f.submit 'submit', :class => "waves-effect waves-light amber lighten-1 white-text btn" %><br></br>
<% end %>
