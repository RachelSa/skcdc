<div id="left-col" class="col l4">
  <h5>Admin Account</h5>

Does <%= @program.title %> no longer exist?<br>
<%= button_to "delete program", @program, :method => :delete, data: {confirm: "Are you sure you want to delete this program?"} %>

</div>

<div id="right-col" class='col l6'>
  <h5>Edit <%= @program.title %></h5>


  <%= form_for @program do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :description %>

<br>

    <%= f.submit %>
  <% end %> <br></br>




</div>
