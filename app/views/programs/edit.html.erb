<div id="left-col" class="col l4 m4 s12 center-align">
  <h5><%= link_to "Admin Account", admin_path %></h5>
  <%= link_to "see published program", @program %>
  <hr class="yellow-rule">
  Delete <%= @program.title %>?<br>
  <%= button_to "delete program", @program, :method => :delete, data: {confirm: "Are you sure you want to delete this program?"}, class: "amber lighten-1 white-text btn" %>
</div>

<div id="right-col" class='col l8 m8 s12'>
  <%= render partial: '/general/error_notice' %>
  <h5 class="yellow-title">Edit <%= @program.title %></h5>
  <%= render partial: 'form' %>
</div>
