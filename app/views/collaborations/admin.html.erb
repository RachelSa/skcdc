<%= render partial: "/general/admin_left_sidebar" %>

<div id="right-col" class="col l8 m8 s12 center-align">
  <h5 class="yellow-title">Edit Collaborations</h5>
  <ul class="collection">
    <li class="collection-item">
      <%= link_to "+ new collaboration", "/collaborations/new" %>
    </li>
    <% @collaboration_titles.each do |collaboration| %>
      <li class="collection-item">
        <%= link_to "#{collaboration[0]}", "/collaborations/#{collaboration[1]}/edit" %>
      </li>
    <% end %>
    <br></br>
    <p><%= link_to "◁ return to admin main page", '/admin' %></p>
</div>
