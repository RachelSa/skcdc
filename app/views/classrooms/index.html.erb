
<div id="left-col" class="col l4 m4">
    <%= render file: "/general/_logo" %>
    <p>add classrooms map</p>
</div>


<div id="right-col" class="col l7 m7">
  <h2>Our Classrooms</h2>
  <div class="row">
    <% @classrooms.each do |classroom| %>
    <div class="col s12 m12 l10">
      <div class="card">
        <div class="card-content blue-grey-text center-align">
          <div><%= link_to "#{classroom.name}", classroom %></div>
          <div><%= classroom.location %></div>
        </div>
      </div>
    </div>
      <% end %>
</div>

</div>
